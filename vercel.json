{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server.js" }, 
      { "src": "/(.*)", "dest": "/frontend/package.json" } 
    ]
  }
  